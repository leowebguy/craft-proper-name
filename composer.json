{
  "name": "leowebguy/proper-name",
  "description": "Plugin intended to prevent biased, copyrighted and other not desired/recommended assets naming.",
  "keywords": [
    "craftcms",
    "assets",
    "names",
    "naming"
  ],
  "version": "1.0.2",
  "type": "craft-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo",
      "homepage": "https://github.com/leowebguy"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^3.0|^4.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "leowebguy\\propername\\": "src/"
    }
  },
  "extra": {
    "name": "Proper Name",
    "handle": "proper-name",
    "hasCpSection": false,
    "hasCpSettings": true,
    "components": {
      "propernameService": "leowebguy\\propername\\services\\ProperNameService"
    },
    "class": "leowebguy\\propername\\ProperName"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "check-cs": "vendor/bin/ecs check src --ansi",
    "fix-cs": "vendor/bin/ecs check src --ansi --fix",
    "phpstan": "vendor/bin/phpstan analyse src"
  }
}
